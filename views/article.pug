extends layout

block content
  h4= article.activationcode
  h5 Belongs to #{article.company}
  h6 #{article.status}
  h6 Expiration date is on #{article.expirationdate}
  h6 #{article.laserhead}
  h6 #{article.datalogger}
  h6 #{article.camera}
  p= article.user
  hr
  a.btn.btn-default.border(href='/article/edit/'+article._id) Edit
  
  a.btn.btn-danger.delete-article(href='#', data-id=article._id) Delete
